block attach, elem control {

    tag: 'input'

    mix: {
        block: 'button', elem: 'control'
    }

    attrs: {
        var a = {
                value: '',
                type: 'file',
                id: this._id,
                name: this._name,
                tabindex : this._tabindex
            };

        !!this._disabled && (a.disabled = 'disabled');
        //this._tabindex && (a.tabindex = this._tabindex);

        return a;
    }

}
